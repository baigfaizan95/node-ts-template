{
  "name": "node-ts-template",
  "version": "1.0.0",
  "scripts": {
    "start": "nodemon ./build/server.js",
    "build": "gulp && npm run tslint && tsc",
    "build:watch": "gulp && tsc -w && npm run tslint && gulp copy-template",
    "dev-server": "concurrently -k -p '[{index}][{name}] -> ' -c 'white.bold,red.bold' 'npm:build:watch' 'npm:start'",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "prod": "npm run build && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1"
